/* pages/search/search.wxss */
#search{
  font-size:12px; 
  padding: 20rpx 10rpx;
}
.search_btn{
  height: 70rpx;
}
input{
  width: 100%;
  height: 70rpx;
  padding: 0 0 0 12px;
  border-bottom: 1px solid #c5c5c5;
  box-sizing: border-box;
}
.nodata{
  margin: 50rpx 0;
  text-align: center
}
.search_title{
  padding: 20rpx 0 20rpx 10px;
}
.rankimg{
  width: 30%;
}
.search_item{
  padding: 20rpx;
  border-bottom:1px solid #c5c5c5;
}
.rank_right{
  width: 65%;
  margin-left:5%; 
  display: inline-block;
  vertical-align: top;
}
.rank_info{
  display: block;
  word-break: break-all;
  margin-bottom: 15rpx;
}